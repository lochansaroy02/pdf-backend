{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "app/main.py"
        }
    ],
    "functions": {
        "app/main.py": {
            "memory": 3008,
            "maxDuration": 60
        }
    }
}